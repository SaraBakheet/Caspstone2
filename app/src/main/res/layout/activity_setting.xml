<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back6"
    android:backgroundTint="#FFFFFF"

    android:tooltipText="30dp"
    tools:context=".Setting">


    <ImageView
        android:id="@+id/logOut"
        android:layout_width="98dp"
        android:layout_height="90dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="123dp"
        android:layout_marginEnd="50dp"
        android:layout_marginBottom="424dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/call"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Setting0"
        app:srcCompat="@drawable/logout1" />

    <ImageView
        android:id="@+id/delete"
        android:layout_width="70dp"
        android:layout_height="68dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="227dp"
        android:layout_marginEnd="44dp"
        android:layout_marginBottom="436dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/call"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/delete1" />

    <TextView
        android:id="@+id/Setting0"
        android:layout_width="317dp"
        android:layout_height="86dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="28dp"
        android:text="@string/setting"
        android:textColor="#204251"
        android:textSize="28dp"
        android:textStyle="italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/chang_lan"
        android:layout_width="273dp"
        android:layout_height="68dp"
        android:layout_marginStart="94dp"
        android:layout_marginTop="46dp"

        android:layout_marginEnd="100dp"
        android:layout_marginBottom="31dp"
        android:background="@drawable/sub"
        android:text="@string/language"
        android:textColor="#090909"
        android:textSize="19dp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/call"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="377dp"
        android:layout_height="202dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="63dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/back99" />

    <ImageView
        android:id="@+id/call"
        android:layout_width="42dp"
        android:layout_height="50dp"
        android:layout_marginStart="184dp"
        android:layout_marginTop="245dp"
        android:layout_marginEnd="185dp"
        android:layout_marginBottom="46dp"
        app:layout_constraintBottom_toTopOf="@+id/chang_lan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Setting0"
        app:srcCompat="@drawable/call9" />

</androidx.constraintlayout.widget.ConstraintLayout>